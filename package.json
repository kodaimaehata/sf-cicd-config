{
	"name": "sf-cicd-config",
	"description": "SF CICD Config - A Webview for configure package.xml and build.xml for sf cicd pipeline",
	"version": "0.0.1",
	"publisher": "kodai",
	"engines": {
		"vscode": "^1.26.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:SF-CICD-Config.openConfigScreen",
		"onWebviewPanel:SF-CICD-Config"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "SF-CICD-Config.openConfigScreen",
				"title": "Open Config Screen",
				"category": "SF-CICD-Config"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -p ./",
		"watch": "tsc -w -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"dependencies": {
		"@types/node": "^10.5.2",
		"jquery": "^3.5.0",
		"xml2js": "^0.4.19"
	},
	"devDependencies": {
		"vscode": "^1.1.34",
		"tslint": "^5.11.0",
		"typescript": "^2.9.2"
	}
}
